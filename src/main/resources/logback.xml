<configuration>
        <appender name="FILE" class="ch.qos.logback.core.ConsoleAppender">
                <encoder>
                        <pattern>%d %-5level %logger{35} - %msg %n</pattern>
                </encoder>
        </appender>
        <root level="info">
                <appender-ref ref="FILE" />
        </root>
</configuration>
